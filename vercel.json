{
  "version": 2,
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/index"
    },
    {
      "source": "/sse",
      "destination": "/api/index"
    },
    {
      "source": "/.well-known/oauth-authorization-server",
      "destination": "/api/index"
    },
    {
      "source": "/oauth-authorization-server",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}

