{
  "name": "famulor-mcp",
  "version": "0.1.0",
  "description": "MCP Server for Famulor Voice Agent Platform - AI-powered phone calling and voice assistant management",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "vercel-build": "npm run build",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "famulor",
    "voice-agent",
    "ai-calling",
    "phone-automation",
    "chatgpt",
    "voice-assistant"
  ],
  "author": "bekservice",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
